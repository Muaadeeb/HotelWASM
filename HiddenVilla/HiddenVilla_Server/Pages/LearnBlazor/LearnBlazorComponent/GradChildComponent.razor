<div class="border mt-2">
    <div class="row">
        <h4 class="text-primary pl-4 pt-2 col-12">Grand Child Component</h4>
    </div>
    <div class="row">
        <p class="pl-4 ml-2">There is a message - @_messageForGrandChild </p>
        <p class="pl-4 ml-2">Lukcy Number - @_luckyNumberFromGrandParent </p>
    </div>
</div>



@code {

    [CascadingParameter(Name = "MessageFromGrandParent")]
    private string _messageForGrandChild { get; set; }

    [CascadingParameter (Name = "GrandParentsLuckyNumber")]
    private int _luckyNumberFromGrandParent { get; set; }

}
